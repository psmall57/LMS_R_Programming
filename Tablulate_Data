table(OzHi = Ozone > 80, Month)

table(OzHi = Ozone > 80, Month, useNA="ifany") #shows how many NA values are in the dataset

table(OzHi = Ozone > 80, Month, useNA="always") #always option will force a row even if there are no missing data points

